{
  "provider": "uspto_odp_pfw",
  "name": "USPTO Open Data Portal â€” Patent File Wrapper: Lookup Application",
  "version": "2025-11-11",
  "base_url": "https://api.uspto.gov",
  "auth": {
    "type": "apiKey",
    "in": "header",
    "name": "X-API-KEY",
    "env_var": "USPTO_ODP_API_KEY"
  },
  "docs": {
    "swagger": "https://data.uspto.gov/swagger/index.html",
    "guide": "https://data.uspto.gov/apis/patent-file-wrapper/search"
  },
  "operation": {
    "op_id": "pfw.get_application",
    "summary": "Get a specific application by application number",
    "method": "GET",
    "path": "/api/v1/patent/applications/{applicationNumberText}",
    "params": {
      "path": {
        "applicationNumberText": {
          "type": "string",
          "example": "14412875"
        }
      }
    },
    "response": {
      "schema_hint": "See patent-data-schema.json -> patentFileWrapperDataBag[0]"
    }
  }
}